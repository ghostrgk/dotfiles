set is

inoremap jk <ESC>
colorscheme torte

filetype plugin indent on

set t_Co=256
set encoding=utf-8
syntax on
set laststatus=2
set showcmd
set rnu nu

let g:netrw_banner = 0
let g:netrw_liststyle = 3
let g:netrw_winsize = 20
let g:netrw_altv = 1

try
    set completeopt=menuone,noinsert,popup
catch /E474/
    " popup is unsupported sometimes
    set completeopt=menuone,noinsert
endtry